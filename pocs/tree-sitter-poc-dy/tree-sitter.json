{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "dy",
      "highlights": "queries/highlights.scm",
      "camelcase": "DY",
      "scope": "source.dy",
      "file-types": [
        "dy"
      ],
      "injection-regex": "^dy$",
      "class-name": "TreeSitterDy"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "All rights reserved",
    "description": "A human-writable data format generator",
    "authors": [
      {
        "name": "Samuel Roland"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-dy"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
